<div class="container">
  <!-- Heading -->
  <div class="row">
    <div class="col-lg-12">
      <h1 class="page-header">Travel Australia
        <small>city guides in one place</small>
      </h1>
    </div>
  </div>
  <!-- /.row -->

  <div class="row">
    {{#each cities}}
    <div class="col-md-4 portfolio-item">
      <a href="{{url}}">
        <img class="img-responsive" src="{{image}}" alt="{{cityName}}">
      </a>
      <h3>
        <a href="{{url}}">{{cityName}}, {{state}}</a>
      </h3>
      <p>{{description}}</p>
    </div>
    {{/each}}
  </div>
</div>
